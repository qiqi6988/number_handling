CountLinesServer_ioc_notrap: CountLinesServer.o
	clang -o CountLinesServer_ioc_notrap CountLinesServer.o  -fioc-explicit-conversion -fioc-implicit-conversion -fioc-unsigned -fioc-signed -fioc-shifts -fioc-strict-shifts 
CountLinesServer.o: CountLinesServer.c
	clang -c CountLinesServer.c  -fioc-explicit-conversion -fioc-implicit-conversion -fioc-unsigned -fioc-signed -fioc-shifts -fioc-strict-shifts

clean: 
	rm -f *.o


