
recaman_ioc: oneshotsrv.o recaman.o
	clang -o recaman_ioc recaman.o oneshotsrv.o -fioc-abort-on-error -fioc-explicit-conversion -fioc-implicit-conversion -fioc-unsigned -fioc-signed -fioc-shifts -fioc-strict-shifts

recaman.o: recaman.c
	clang -c recaman.c -fioc-abort-on-error -fioc-explicit-conversion -fioc-implicit-conversion -fioc-unsigned -fioc-signed -fioc-shifts -fioc-strict-shifts

oneshotsrv.o: oneshotsrv.c 
	clang -c oneshotsrv.c -fioc-abort-on-error -fioc-explicit-conversion -fioc-implicit-conversion -fioc-unsigned -fioc-signed -fioc-shifts -fioc-strict-shifts
clean: 
	rm -f *.o
