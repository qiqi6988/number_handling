

countlines_ioc: query.o countlines.o
	clang -o countlines_ioc countlines.o query.o -fioc-abort-on-error -fioc-explicit-conversion -fioc-implicit-conversion -fioc-unsigned -fioc-signed -fioc-shifts -fioc-strict-shifts
countlines.o: countlines.c
	clang -c countlines.c query.h -fioc-abort-on-error -fioc-explicit-conversion -fioc-implicit-conversion -fioc-unsigned -fioc-signed -fioc-shifts -fioc-strict-shifts
query.o: query.c
	clang -c query.c query.h -fioc-abort-on-error -fioc-explicit-conversion -fioc-implicit-conversion -fioc-unsigned -fioc-signed -fioc-shifts -fioc-strict-shifts

clean: 
	rm -f *.o
