

countlines_ioc_notrap: countlines.o
	clang -o countlines_ioc_notrap countlines.o -fioc-explicit-conversion -fioc-implicit-conversion -fioc-unsigned -fioc-signed -fioc-shifts -fioc-strict-shifts
countlines.o: countlines.c 
	clang -c countlines.c -fioc-explicit-conversion -fioc-implicit-conversion -fioc-unsigned -fioc-signed -fioc-shifts -fioc-strict-shifts
clean: 
	rm -f *.o
